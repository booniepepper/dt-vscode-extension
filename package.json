{
    "name": "dt",
    "displayName": "dt",
    "description": "dt syntax highlighting.",
    "version": "0.1.0",
    "repository": "https://github.com/hiljusti/dt-vscode-extension",
    "publisher": "hiljusti",
    "engines": {
        "vscode": "^1.60.0"
    },
    "categories": [
        "Programming Languages"
    ],
    "contributes": {
        "languages": [{
            "id": "dt",
            "aliases": ["duck tape", "duct tape"],
            "extensions": [".dt"],
            "configuration": "./language-configuration.json"
        }],
        "grammars": [{
            "language": "dt",
            "scopeName": "source.dt",
            "path": "./syntaxes/dt.tmLanguage.json"
        }]
    }
}
