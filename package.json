{
    "name": "dt",
    "displayName": "dt",
    "description": "dt syntax highlighting.",
    "version": "1.0.2",
    "icon": "icon.png",
    "repository": "https://github.com/booniepepper/dt-vscode-extension",
    "publisher": "booniepepper",
    "engines": {
        "vscode": "^1.60.0"
    },
    "categories": [
        "Programming Languages"
    ],
    "contributes": {
        "languages": [
            {
                "id": "dt",
                "aliases": [
                    "duct tape",
                    "duck tape"
                ],
                "extensions": [
                    ".dt"
                ],
                "configuration": "./language-configuration.json"
            }
        ],
        "grammars": [
            {
                "language": "dt",
                "scopeName": "source.dt",
                "path": "./syntaxes/dt.tmLanguage.json"
            }
        ]
    }
}
